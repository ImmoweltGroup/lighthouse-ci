{
  "name": "@immowelt/lighthouse-ci",
  "version": "0.0.0-development",
  "main": "index.js",
  "repository": "https://github.com/ImmoweltGroup/lighthouse-ci.git",
  "author": "Kevin Ruff <kevin.ruff@immonet.de>",
  "license": "MIT",
  "files": [
    "bin",
    "src/*"
  ],
  "engines" : {
    "node": ">=8.11.4",
    "yarn": ">=1.6.0"
  },
  "bin": {
    "lighthouse-ci": "bin/ci.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master",
    "analyzeCommits": {
      "releaseRules": [
        {
          "type": "BREAKING",
          "release": "major"
        },
        {
          "type": "FEATURE",
          "release": "minor"
        },
        {
          "type": "BUGFIX",
          "release": "patch"
        },
        {
          "type": "SECURITY",
          "release": "patch"
        }
      ]
    }
  },
  "dependencies": {
    "bluebird": "3.5.2",
    "lighthouse": "3.2.1",
    "lodash.flattendeep": "^4.4.0",
    "lodash.pick": "4.4.0",
    "signale": "^1.2.1",
    "yargs": "12.0.2"
  },
  "keywords": [
    "lighthouse",
    "ci",
    "thresoulds"
  ],
  "scripts": {
    "start": "node ./bin/ci.js",
    "lint": "standard",
    "release": "semantic-release",
    "test": "jest src"
  },
  "devDependencies": {
    "@semantic-release/github": "5.2.1",
    "@semantic-release/npm": "5.0.6",
    "jest": "23.6.0",
    "semantic-release": "15.9.12",
    "standard": "12.0.1"
  },
  "standard": {
    "env": [ "jest" ]
  }
}
